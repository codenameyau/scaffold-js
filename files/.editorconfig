# Top-most editorconfig
root = true

# Unix-style newlines
[*]
end_of_line = lf
insert_final_newline = true

# JavaScript specific
[*.js]
indent_style = space
indent_size = 2
charset = utf-8

# Matches the exact files either package.json or .travis.yml
[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
